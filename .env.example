# Настройки авторизации и URL МТУСИ
# =============================
MTUCI_EMAIL='lk.mtuci.email@example.com'
MTUCI_PASSWORD='lk-mtuci-password'
MTUCI_BASE_URL=https://lk.mtuci.ru
MTUCI_SCHEDULE_URL=https://lk.mtuci.ru/student/schedule

# Настройки Google Calendar
# ========================
# ID календаря из Google Calendar
GOOGLE_CALENDAR_ID='google-calendar-id'
# Путь к файлу с учетными данными от Google Cloud Console
GOOGLE_CREDENTIALS_PATH=credentials.json
# Путь к файлу с токеном для OAuth2
GOOGLE_TOKEN_PATH=token.json
# Название календаря в интерфейсе
GOOGLE_CALENDAR_NAME='МТУСИ Расписание'

# Настройки скрапинга
# ==================
# Максимальное количество попыток при ошибках
SCRAPING_MAX_RETRIES=3
# Таймаут ожидания ответа в миллисекундах
SCRAPING_TIMEOUT_MS=15000
# Корпус по умолчанию (Н - Народного Ополчения, А - Авиамоторная)
SCRAPING_DEFAULT_BUILDING=Н
# Группа по умолчанию
SCRAPING_DEFAULT_GROUP=БИК2404
